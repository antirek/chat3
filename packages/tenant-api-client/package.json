{
  "name": "@chottodev/chat3-tenant-api-client",
  "version": "0.0.40",
  "description": "TypeScript/JavaScript client library for Chat3 Tenant API",
  "main": "dist/Chat3Client.js",
  "types": "dist/Chat3Client.d.ts",
  "type": "commonjs",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --coverage",
    "test:integration": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js Chat3Client.integration.test.js",
    "test:unit": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --testPathIgnorePatterns=integration",
    "test:with-api": "./test-with-api.sh",
    "prepublishOnly": "npm run test:unit",
    "prepack": "npm run test:unit",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "keywords": [
    "chat3",
    "chat",
    "messaging",
    "api",
    "client",
    "sdk"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "axios-logger": "^2.8.1"
  },
  "devDependencies": {
    "jest": "^30.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antirek/chat3.git",
    "directory": "packages/tenant-api-client"
  },
  "publishConfig": {
    "access": "public"
  }
}
