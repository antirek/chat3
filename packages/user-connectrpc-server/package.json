{
  "name": "@chat3/user-connectrpc-server",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsc --watch & node --watch dist/index.js",
    "generate:proto": "cd ../../packages-shared/proto && buf generate",
    "generate:openapi": "protoc --plugin=protoc-gen-connect-openapi=./node_modules/.bin/protoc-gen-connect-openapi --connect-openapi_out=. --proto_path=../../packages-shared/proto/src ../../packages-shared/proto/src/chat3_user.proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.1",
    "@chottodev/chat3-tenant-api-client": "*",
    "@connectrpc/connect": "^1.7.0",
    "@connectrpc/connect-express": "^1.7.0",
    "@connectrpc/connect-node": "^1.7.0",
    "amqplib": "^0.10.9",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "^1.10.1",
    "@connectrpc/protoc-gen-connect-es": "^1.7.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.27",
    "@types/swagger-ui-express": "^4.1.6",
    "typescript": "^5.9.3"
  }
}
