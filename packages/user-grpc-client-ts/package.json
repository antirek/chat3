{
  "name": "@chat3/user-grpc-client-ts",
  "version": "0.0.26",
  "description": "TypeScript client library for Chat3 gRPC User Service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "proto",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "generate:proto": "protoc --plugin=protoc-gen-ts_proto --ts_proto_out=./src/generated --proto_path=./proto ./proto/chat3_user.proto",
    "test": "node test-grpc.js"
  },
  "keywords": [
    "chat3",
    "grpc",
    "client",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.11.0",
    "@grpc/proto-loader": "^0.7.13"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "typescript": "^5.9.3"
  }
}
